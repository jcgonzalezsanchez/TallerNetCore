@page
@model TGL.WebApp.Pages.Computers.AddModel
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>

<form method="post">
    <input hidden asp-for="StudentId" />
    Brand
    <input asp-for="Computer.Brand" />
    Model
    <input asp-for="Computer.Model" />
    Cpu
    <input asp-for="Computer.Cpu" />
    Ram
    <input asp-for="Computer.Ram" />

    <button type="submit" class="btn">
        Save
    </button>
</form>

<a asp-page="./Index">Return to list</a>

